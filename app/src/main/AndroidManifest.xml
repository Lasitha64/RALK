<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ralk" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RALK" >
        <activity
            android:name=".NavActivity"
            android:exported="true"
            android:label="@string/title_activity_nav_main"
            android:theme="@style/Theme.RALK.NoActionBar" />
        <activity
            android:name=".cart"
            android:exported="true" />
        <activity
            android:name=".food_list"
            android:exported="true" />
        <activity
            android:name=".cus.customer_main"
            android:exported="true"
            android:label="@string/title_activity_customer_main_view"
            android:theme="@style/Theme.RALK.NoActionBar" />
        <activity android:name=".cus.activity_customer_login" />
        <activity android:name=".admin.adminMain" />
        <activity android:name=".admin.addcatadmin" />
        <activity android:name=".admin.adminList" />
        <activity android:name=".OrderView" />
        <activity android:name=".cus.activity_customer_signup" />
        <activity android:name=".deliver_profile" />
        <activity android:name=".LoginDeliveryManager" />
        <activity android:name=".FoodDetails" />

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>